{
    "routes": [
        { "handle": "filesystem" },
        { "src": "/demo/?", "dest": "/api/demo/index.php" },
        { "src": "/demo/(.+)", "dest": "/api/demo/vercel-static.php" },
        { "src": "/", "dest": "/api/index.php" }
    ],
    "functions": {
        "api/**/*": {
            "runtime": "vercel-php@0.6.0"
        }
    }
}
